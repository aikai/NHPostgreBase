		<property name="TpdComment" type="String" column="`TPD_COMMENT`" length="255" />
    <property name="CreateBy" type="String" column="`CREATE_BY`" length="10" />
    <property name="CreateDate" type="DateTime" column="`CREATE_DATE`" length="7" />
